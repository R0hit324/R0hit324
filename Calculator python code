num1=float(input("Enter number1:"))
operator=input("Select any operator \"+,-,*,/\":")
num2=float(input("Enter number2:"))

if(operator=="+"):
    print(f"{num1} + {num2} =",num1+num2)
elif(operator=="-"):
    print(f"{num1} - {num2} =",num1-num2)
elif(operator=="*"):
    print(f"{num1} * {num2} =",num1*num2)
elif(operator=="/"):
    print(f"{num1} / {num2} =",num1/num2)

